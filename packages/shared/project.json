{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "shared",
  "projectType": "library",
  "sourceRoot": "packages/shared/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/shared"],
      "options": {
        "command": "tsc -p packages/shared/tsconfig.json"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p packages/shared/tsconfig.json"
      }
    }
  },
  "tags": ["type:shared", "scope:shared"]
}
